/* pages/function/control/fault-detail/fault-detail.wxss */
.page-body {
  background-color: #f6f6f6;
  min-height: 100vh;
}
.peopleList {
  background-color: #fff;
  margin-top: 20rpx;
}

/* 复选框 */
.check-label {
  height: 120rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1rpx solid #dcdcdc;
}
.check-label .check-img {
  display: flex;
  align-items: center;
}
.check-label .check-img image{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}
.check-label .check-bd {
  font-size: 35rpx;
  margin-left: 25rpx;
}

.collapse-item {
  min-height: 75rpx;
}
.collapse-item-title {
  font-size: 35rpx;
}
.button {
  margin-top: 100rpx;
  height: 200rpx;
}
.button button{
  background-color: #1296db;
  margin: 0 50rpx;
  border-radius: 100rpx;
  color: #fff;
}