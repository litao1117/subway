/* pages/mine/mine.wxss */
page {
  /* background-color: #f6f6f6; */
}
.page-body {
  flex: 1;
}
.login {
  position: absolute;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center; 
  width: 100%;
  height: 533rpx;
  z-index: 1;
}
.avatar {
  height: 220rpx;
  width: 220rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-top: 20rpx;
}
.avatar image {
  width: 220rpx;
  height: 220rpx;
  border-radius: 50%;
}
.loginState {
  width: 244rpx;
  text-align: center;
}
.loginState .textone {
  font-size: 40rpx;
  color: #222222;
}
.loginState .texttwo {
  font-size: 24rpx;
  color: #888888;
  margin-top: 26rpx;
}
.loginButton {
  display: flex;
  flex-direction: column;
  height: 244rpx;
  width: 200rpx;
  justify-content: center;
  align-items: center;
}
.buttonlog {
  text-align: center;
  line-height: 75rpx;
  height: 75rpx;
  width: 350rpx;
  font-size: 30rpx;
  border-radius: 42rpx;
  border:2rpx solid #fff;
  background-color: #fff;
  color :#49ADD6;
}

/* 头像姓名 */
.bgBox {
  width: 100%;
  height: 533rpx;
  position: relative;
}
.bgImg {
  position: absolute;
  width: 100%;
  height: 533rpx;
}
.topBox {
  position: absolute;
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 100%;
  height: 533rpx;
  align-items: center;
  z-index: 1;
}
.textInfo {
  text-align: center;
  width: 400rpx;
  height: 120rpx;
  margin-top: 50rpx;
}
.nickname {
  font-size: 32rpx;
  color: #222222;
}
.sexIcon {
  width: 30rpx;
  height: 30rpx;
}
.idNum {
  font-size: 28rpx;
  margin-top: 10rpx;
}
.myButton {
  background-color: #fff;
  border-color: #fff;
}
/* 功能块 */
.funcBox {
  width: 100%;
  /* height: 812rpx; */
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.funcSmall {
  width: 371rpx;
  height: 264rpx;
  font-size: 28rpx;
  border-bottom: 2rpx solid #E7E9F8;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.rightBorder {
  border-right: 2rpx solid #E7E9F8;
}
.funcIcon {
  margin-bottom: 28rpx;
  /* background-size: cover; */
}
